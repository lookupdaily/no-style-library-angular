{
  "name": "no-styles-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "dev:ssr": "ng run no-styles-angular:serve-ssr",
    "serve:ssr": "node dist/no-styles-angular/server/main.js",
    "build:ssr": "ng build && ng run no-styles-angular:server",
    "prerender": "ng run no-styles-angular:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.11",
    "@angular/common": "^17.3.11",
    "@angular/compiler": "^17.3.11",
    "@angular/core": "^17.3.11",
    "@angular/forms": "^17.3.11",
    "@angular/platform-browser": "^17.3.11",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@angular/platform-server": "^17.3.11",
    "@angular/router": "^17.3.11",
    "@angular/ssr": "^17.3.8",
    "@testing-library/jest-dom": "^6.4.6",
    "express": "^4.18.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.11",
    "@testing-library/angular": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.18.0",
    "browser-sync": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.1.0",
    "ng-packagr": "^17.3.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  }
}